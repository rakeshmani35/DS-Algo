package com.dp;

// https://www.youtube.com/watch?v=rMLEZdtdAO8&list=PLCZJSEiJwWZ7kd4wXxIkFiU6mYXsCwYOC&index=11
public class NStairsProblem {

	public static int countWays(int stair, int jump) {

		if (stair <= jump) {
			return stair;
		}
		return jump;
	}

	public static void main(String[] args) {

	}

}
